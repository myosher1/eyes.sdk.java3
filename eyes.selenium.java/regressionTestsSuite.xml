<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="regression suite">

    <listeners>
        <listener class-name="com.applitools.eyes.selenium.ExcludeFailingTestsListener"/>
        <listener class-name="com.applitools.eyes.selenium.RetryListener" />
        <listener class-name="com.applitools.eyes.selenium.PostTestResultsListener" />
    </listeners>

    <suite-files>
        <suite-file path="passedTestsSuite.xml" />
        <suite-file path="mobileTestsSuite.xml" />
    </suite-files>
</suite>